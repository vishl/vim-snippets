
snippet if "if"
<% if(${1:condition}){%>
  $2
<% } %>
endsnippet

snippet else
<% if(${1:condition}){%>
  $2
<% }else{ %>
<% } %>
endsnippet

snippet %
<% $1 %>
endsnippet

snippet = "<%- ${0} %>"
<%- ${1} %>
endsnippet

snippet - "<%- ${0} %>"
<%- ${1} %>
endsnippet

snippet each 
<% _.each($1, function(${2:e}){ %>
  $3
<% }); %>
endsnippet

snippet img
<img src="<%- $1 %>"$2>
endsnippet

snippet JST
<%= JST['backbone/templates/$1']($2) %>
endsnippet
